  "properties": {
    "exception_uuid": {
      "title": "Exception Uuid",
      "type": "string"
    },
    "po_number": {
      "title": "Po Number",
      "type": "string"
    },
    "sender_information": {
      "$ref": "#/$defs/ContactInformation"
    },
    "master_data_errors": {
      "allOf": [
        {
          "$ref": "#/$defs/MasterDataErrors"
        }
      ],
      "default": {}
    },
    "exceptions": {
      "allOf": [
        {
          "$ref": "#/$defs/Exceptions"
        }
      ],
      "default": {}
    },
    "other_errors": {
      "anyOf": [
        {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Other Errors"
    }
  },
  "required": [
    "exception_uuid",
    "po_number",
    "sender_information"
  ],
  "title": "ExceptionReport",
  "type": "object"
}
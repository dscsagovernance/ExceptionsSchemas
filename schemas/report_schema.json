        "missing_product": {
          "items": {
            "$ref": "#/$defs/EPCISNoProductDescription"
          },
          "title": "Missing Product",
          "type": "array"
        },
        "overages": {
          "items": {
            "$ref": "#/$defs/OveragesUnderages"
          },
          "title": "Overages",
          "type": "array"
        },
        "underages": {
          "items": {
            "$ref": "#/$defs/OveragesUnderages"
          },
          "title": "Underages",
          "type": "array"
        }
      },
      "title": "Exceptions",
      "type": "object"
    },
    "GLNError": {
      "properties": {
        "exception_item_uuid": {
          "title": "Exception Item Uuid",
          "type": "string"
        },
        "invalid_gln": {
          "title": "Invalid Gln",
          "type": "string"
        },
        "proposed_gln": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }

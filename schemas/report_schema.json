      "type": "object"
    },
    "ProductNoEPCISDescription": {
      "properties": {
        "exception_item_uuid": {
          "title": "Exception Item Uuid",
          "type": "string"
        },
        "product_gtin": {
          "title": "Product Gtin",
          "type": "string"
        },
        "observation_source": {
          "$ref": "#/$defs/ObservationSource"
        },
        "resolution_request": {
          "$ref": "#/$defs/Resolution"
        },
        "product_serial": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Product Serial"
        }
      },
      "required": [
        "exception_item_uuid",
        "product_gtin",
        "observation_source",
        "resolution_request"
      ],
      "title": "ProductNoEPCISDescription",
      "type": "object"
    },
    "Resolution": {
      "enum": [
        "Send new data",
        "Send product",
        "Return product",
        "Send back product",
        "Credit account",
        "Debit account"

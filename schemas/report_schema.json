          "type": "string"
        },
        "file_id": {
          "title": "File Id",
          "type": "string"
        },
        "product_gtin": {
          "title": "Product Gtin",
          "type": "string"
        },
        "product_serial": {
          "title": "Product Serial",
          "type": "string"
